@{
    ViewBag.Title = "RobotTask";
}

<div class="row">
    <h2>Robot Toy Direction</h2>
</div>

<div class="row">
    <div class="col-md-12">
        <div class="col-md-2">
            X-Coordiante
        </div>
        <div class="col-md-2">
            <input type="number" value="3" class="text-primary" id="XId" onblur="ProcessXAxis()" />
        </div>
        <div class="col-md-2">
            Y-Coordiante
        </div>
        <div class="col-md-2">
            <input type="number" value="3" class="text-primary" id="YId" onblur="ProcessYAxis()" />
        </div>
    </div>
</div>
<div class="row">
