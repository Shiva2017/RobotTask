        <div class="col-md-2">
            Face
        </div>
        <div class="col-md-2">
            <select id="ddlFace" name="ddlFace" class="dropdown-header" onchange="FaceChange()">
                <option value="1">North</option>
                <option value="2">East</option>
                <option value="3">South</option>
                <option value="4">West</option>
            </select>
        </div>
        <div class="col-md-4">
            <input type="button" class="btn btn-primary" value="Left" onclick="ProcessLeft()" />
            <input type="button" class="btn btn-primary" value="Move" onclick="ProcessMove()" />
            <input type="button" class="btn btn-primary" value="Right" onclick="ProcessRight()" />
        </div>
        <div class="col-md-4">
            <img src="~/Image/Directions.png" style="width:100px;height:100px"/>
        </div>
    </div>
</div>
